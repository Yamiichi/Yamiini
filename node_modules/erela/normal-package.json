{
    "name": "erela-application",
    "version": "1.0.0",
    "description": "",
    "repository": {
      "type": "git",
      "url": "https://github.com/MohammedAl-Rowad/Erela"
    },
    "scripts": {
      "dev": "ts-node -r tsconfig-paths/register src/app.ts --watch",
      "lintme": "tslint",
      "build": "rm -rf dist && mkdir dist && cp .env dist && tsc",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
      "typeorm:migrate": "npm run typeorm -- migration:generate -n",
      "typeorm:run": "npm run typeorm migration:run",
      "create:entity": "npm run typeorm -- entity:create -d src/entities -n",
      "create:sub": "npm run typeorm -- subscriber:create -d src/subscribers -n"
    },
    "_moduleAliases": {
      "@types": "src/types",
      "@controllers": "src/controllers",
      "@entities": "src/entities",
      "@interfaces": "src/interfaces",
      "@services": "src/services"
    },
    "keywords": [],
    "author": "Rowadz",
    "license": "MIT",
    "devDependencies": {
      "@tsconfig/node12": "^1.0.7",
      "@types/body-parser": "^1.19.0",
      "@types/colors": "^1.2.1",
      "@types/express": "^4.17.8",
      "@types/figlet": "^1.2.1",
      "@types/fs-extra": "^9.0.4",
      "@types/glob": "^7.1.3",
      "@types/inquirer": "^7.3.1",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/multer": "^1.4.4",
      "@types/node": "^14.10.1",
      "@types/ora": "^3.2.0"
    },
    "dependencies": {
      "body-parser": "^1.19.0",
      "class-transformer": "^0.3.1",
      "class-validator": "^0.12.2",
      "colors": "^1.4.0",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "figlet": "^1.5.0",
      "fs-extra": "^9.0.1",
      "glob": "^7.1.6",
      "inquirer": "^7.3.3",
      "jsonwebtoken": "^8.5.1",
      "module-alias": "^2.2.2",
      "multer": "^1.4.2",
      "node-notifier": "^8.0.0",
      "ora": "^5.1.0",
      "reflect-metadata": "^0.1.13",
      "routing-controllers": "^0.9.0-alpha.6",
      "ts-node": "^9.0.0",
      "tsconfig-paths": "^3.9.0",
      "tslint": "^6.1.3",
      "typedi": "^0.8.0",
      "typeorm": "^0.2.26",
      "typescript": "^4.0.2",
      "utility-types": "^3.10.0"
    }
  }
  