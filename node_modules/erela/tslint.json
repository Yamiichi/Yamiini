{
  "env": {
    "node": 1
  },
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "semicolon": false,
    "no-any": true,
    "no-console": {
      "severity": "error",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    },
    "quotemark": [true, "single", "jsx-double"]
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
